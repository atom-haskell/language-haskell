'scopeName': 'source.cabal'
'fileTypes': [
  'cabal'
]
'name': 'Cabal'
'patterns': [

      {
          'match':'(version)\\W*:\\W*([\\d\.]+)'
          'captures':
            '1':
                'name':'keyword.other'
            '2':
                'name':'constant.numeric'
          'name':'version'
      }
      {
          'match':'(\\S+):[^/]'
          'captures':
            '1':
              'name':'keyword.other'
          'name':'cabal-keyword'
      }

      {
        'match':'&&'
        'name':'keyword.other'
      }
      {
          'match':'([><=]+)\\s*([\\d\.]+)'
          'captures':
            '1':
              'name':'keyword.other'
            '2':
              'name':'constant.numeric'

          'name':'cabal-keyword'

      }

      {
          'match': '(benchmark|common|executable|flag|source-repository|test-suite)\\s+(\\S+)'
          'captures':
            '1':
                'name':'entity.name.function'
            '2':
                'name':'support.other'
          'name':'module-type'
      }
      {
          'match':'library'
          'name':'entity.name.function'
      }
      {
          'match':'--.*\\n'
          'name':'comment'
      }
]
